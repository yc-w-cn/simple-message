generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Message {
  id           Int      @id @default(autoincrement())
  clientId     Int      @map("client_id")
  eventName    String   @map("event_name")
  payload      Json
  gmtCreatedAt DateTime @default(now()) @map("gmt_created_at")

  @@index([clientId])
  @@index([eventName])
  @@map("message")
}
